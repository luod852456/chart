<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- ————————————————————新视觉(start)———————————————————— -->

    <!--—————————text_base—————————-->
    <style name="sd_style_text_base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <!--—————————字号（start）—————————-->
    <!--全部中文字符-->
    <style name="sd_style_text_48px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px48dp</item>
    </style>

    <style name="sd_style_text_44px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px44dp</item>
    </style>

    <!--全部中文字符-->
    <style name="sd_style_text_40px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px40dp</item>
    </style>

    <!--大尺寸图片的大标题-->
    <style name="sd_style_text_38px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px38dp</item>
    </style>

    <!--大尺寸图片的大标题-->
    <style name="sd_style_text_36px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px36dp</item>
    </style>

    <!--大尺寸图片的大标题-->
    <style name="sd_style_text_34px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px34dp</item>
    </style>

    <!--list标题、卡片标题、大型按钮类操作文字-->
    <style name="sd_style_text_32px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px32dp</item>
    </style>

    <!--正文文本、按钮类操作文字-->
    <style name="sd_style_text_30px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px30dp</item>
    </style>

    <!--正文文本、按钮类操作文字-->
    <style name="sd_style_text_28px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px28dp</item>
    </style>

    <!--讯息或小titel、小型按钮类操作文字-->
    <style name="sd_style_text_26px" parent="sd_style_text_base">
        <item name="android:textSize">@dimen/px26dp</item>
    </style>

    <!--讯息或小titel、小型按钮类操作文字-->
